          <div id="myModal" class="modal">
            <div class="modal-content">
            <button class="close">&times;</button>
            <p>Please enter a value for the new item.</p>
            </div>
            </div>
            
            <form class="item" action="/" method="post" onsubmit="return validateForm()">
            <div class="row">
            <div class="col-sm-12"> <div class="form-group">
            <label for="exampleFormControlTextarea1">Example textarea</label>
            <textarea class="form-control" id="exampleFormControlTextarea1" name="newItem" rows="3"></textarea>
            </div>
            </div>
            <div class="col-sm-12"> <button class="Size_btn" type="submit" name="list"> Submit</button>
            </div>
            </div>
            
            
            
            </form>
            
            
            <script>
            // Get the modal
            var modal = document.getElementById("myModal");
            
            // Get the close button
            var closeBtn = document.getElementsByClassName("close")[0];
            
            // Get the form
            var form = document.getElementsByClassName("item")[0];
            
            // When the form is submitted, validate the input
            form.onsubmit = function() {
            var newItemInput = document.getElementsByName("newItem")[0];
            if (newItemInput.value === "") {
            // If the input is empty, display the modal
            modal.style.display = "block";
            return false;
            }
            return true;
            }
            
            // When the user clicks on the close button, close the modal
            closeBtn.onclick = function() {
            modal.style.display = "none";
            }
            
            // When the user clicks anywhere outside of the modal, close it
            window.onclick = function(event) {
            if (event.target == modal) {
            modal.style.display = "none";
            }
            }
            </script>
        


          

     



 


   
  
         
        
      
      


